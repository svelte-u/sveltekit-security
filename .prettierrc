{
	"semi": false,
	"singleQuote": false,
	"printWidth": 100,
	"tabWidth": 4,
	"useTabs": true,
	"trailingComma": "all",
	"arrowParens": "always",
	"bracketSpacing": true,
	"bracketSameLine": true,
	"svelteStrictMode": true,
	"svelteAllowShorthand": true,
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
